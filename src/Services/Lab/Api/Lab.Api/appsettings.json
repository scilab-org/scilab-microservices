{
  "AllowedHosts": "*",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "Serilog": {
      "Enable": true,
      "ServiceName": "lab.service",
      "MinimumLevel": {
        "Default": "Information",
        "Override": {
          "Microsoft": "Warning",
          "System": "Information"
        }
      },
      "Console": {
        "Enable": true,
        "Level": "{{CONSOLE_LOG_LEVEL}}"
      },
      "Otlp": {
        "Endpoint": "{{OTLP_ENDPOINT}}"
      }
    }
  },
  "ConnectionStrings": {
    "DbType": "POSTGRESQL",
    "Database": "Server={{DB_HOST}};Port=5432;Database=lab_service_db;User Id={{DB_USER}};Password={{DB_PASSWORD}};Include Error Detail={{INCLUDE_ERROR_DETAIL}};"
  },
  "MinIO": {
    "Endpoint": "{{MINIO_ENDPOINT}}",
    "AccessKey": "{{MINIO_ACCESS_KEY}}",
    "SecretKey": "{{MINIO_SECRET_KEY}}",
    "Secure": false
  },
  "Authentication": {
    "Authority": "{{KEYCLOAK_URL}}/realms/hyper-data-lab-realm",
    "ClientId": "hyper-data-lab-web-client",
    "ClientSecret": "{{WEB_CLIENT_SECRET}}",
    "Audience": "account",
    "Scopes": [ "openid" ],
    "ApiName": "agent.api"
  },
  "AppConfig": {
    "ServiceName": "lab.service",
    "ApiKey": "hyperdatalab.com",
    "IncludeInnerException": false,
    "IncludeExceptionStackTrace": false
  },
  "SwaggerGen": {
    "Enable": false,
    "ContactEmail": "trungnguyen0803forwork@gmail.com",
    "ContactName": "Nguyen Thanh Trung",
    "ContactUrl": "https://www.facebook.com/trungn83",
    "OAuth2RedirectUrl": "{{KEYCLOAK_URL}}/swagger/oauth2-redirect.html"
  },
  "ApiClients": {
    "Keycloak": {
      "BaseUrl": "{{KEYCLOAK_URL}}",
      "Realm": "hyper-data-lab-realm",
      "ClientId": "hyper-data-lab-service-account",
      "ClientSecret": "{{SERVICE_ACCOUNT_CLIENT_SECRET}}",
      "Scopes": [ "openid" ],
      "GrantType": "client_credentials"
    }
  },
  "DistributedTracing": {
    "Enable": true,
    "ServiceName": "lab.service",
    "Source": "lab.service.diagnostics",
    "SamplingRate": 0.25,
    "Prometheus": {
      "Enable": true
    },
    "Otlp": {
      "Endpoint": "{{OTLP_ENDPOINT}}",
      "TimeoutMs": 10000
    }
  }
}
